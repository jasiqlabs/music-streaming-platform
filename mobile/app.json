      "android",
      "web"
    ],
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "splash": {
      "image": "./logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-audio",
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "8de8855e-a352-4212-971a-0e208d01b4ce"
      }
    },
    "android": {
      "package": "com.moinimran.fanapp"
    }
  }
}
